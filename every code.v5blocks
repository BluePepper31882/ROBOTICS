tor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1","id":"partner"},"triportSourcePort":22},{"port":[3],"name":"Flywheel","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio6_1"},"triportSourcePort":22},{"port":[4],"name":"Flywheel2","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio6_1"},"triportSourcePort":22}],"slot":7,"platform":"V5","sdkVersion":"20220726.10.00.00","appVersion":"2.4.5","minVersion":"2.4.0","fileFormat":"1.0.1","icon":"","targetBrainGen":"First","cppStatus":"true","cpp":"// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START V5 MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\ncontroller Controller1 = controller(primary);\ndigital_out PA = digital_out(Brain.ThreeWirePort.A);\ndigital_out PB = digital_out(Brain.ThreeWirePort.B);\nmotor BR = motor(PORT20, ratio18_1, false);\n\nmotor BL = motor(PORT19, ratio18_1, false);\n\nmotor FR = motor(PORT18, ratio18_1, false);\n\nmotor FL = motor(PORT17, ratio18_1, false);\n\ngps GPS16 = gps(PORT16, -6.35, -152.40, mm, 180);\nmotor Intake = motor(PORT1, ratio18_1, false);\n\nmotor Roller = motor(PORT2, ratio18_1, false);\n\nmotor Flywheel = motor(PORT3, ratio6_1, false);\n\nmotor Flywheel2 = motor(PORT4, ratio6_1, false);\n\n\n\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat myVariable, flywheel;\n\n// \"when started\" hat block\nint whenStarted1() {\n  // forever loop allows for the movement to be active the whole time during the drive period\n  while (true) {\n    if (Controller1.Axis3.position() > 0.0) {\n      // when postition 3 on the controller goes forward, the robot will move forward\n      BL.spin(forward);\n      BR.spin(forward);\n      FR.spin(forward);\n      FL.spin(forward);\n    } else if (Controller1.Axis3.position() < 0.0) {\n      // when postition 3 on the controller goes backwards, the robot will move backwards\n      BL.spin(reverse);\n      BR.spin(reverse);\n      FR.spin(reverse);\n      FL.spin(reverse);\n    } else {\n      // when nothing is displaced, the wheels will not move\n      BL.stop();\n      BR.stop();\n      FL.stop();\n      FR.stop();\n    }\n    if (Controller1.Axis1.position() < 0.0) {\n      // when position 1 on the controller goes to the left, the robot will spin left on the yaw axis\n      BL.spin(reverse);\n      BR.spin(forward);\n      FR.spin(forward);\n      FL.spin(reverse);\n    } else if (Controller1.Axis1.position() > 0.0) {\n      // when position 1 on the controller goes to the right, the robot will spin righton the yaw axis\n      BL.spin(forward);\n      BR.spin(reverse);\n      FR.spin(reverse);\n      FL.spin(forward);\n    } else {\n      // when nothing is displaced, the wheels will not move\n      BL.stop();\n      BR.stop();\n      FL.stop();\n      FR.stop();\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted2() {\n  // movement of robot\n  // controller position allows for all direction movement\n  FL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n  FL.spin(forward);\n  BL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n  BL.spin(forward);\n  FR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n  FR.spin(forward);\n  BR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n  BR.spin(forward);\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted3() {\n  while (true) {\n    // movement of robot\n    // controller position allows for all direction movement\n    FL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    FL.spin(forward);\n    BL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    BL.spin(forward);\n    FR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    FR.spin(forward);\n    BR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    BR.spin(forward);\n    if (Controller1.ButtonR2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move into the robot\n      Intake.spin(forward);\n    } else if (Controller1.ButtonL2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move away from the robot\n      Intake.spin(reverse);\n    } else {\n      // when nothing is pressed, the intake wheels won't move\n      Intake.stop();\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted4() {\n  while (true) {\n    // movement of robot\n    // controller position allows for all direction movement\n    FL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    FL.spin(forward);\n    BL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    BL.spin(forward);\n    FR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    FR.spin(forward);\n    BR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    BR.spin(forward);\n    if (Controller1.ButtonR2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move into the robot\n      Intake.spin(forward);\n    } else if (Controller1.ButtonL2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move away from the robot\n      Intake.spin(reverse);\n    } else {\n      // when nothing is pressed, the intake wheels won't move\n      Intake.stop();\n    }\n    if (Controller1.ButtonA.pressing()) {\n      // When A is pressed on the controller, the wheels will spin away from the robot\n      Roller.spin(forward);\n    } else if (Controller1.ButtonB.pressing()) {\n      // When B is press on the controller, the wheels will spin into the robot\n      Roller.spin(reverse);\n    } else {\n      // when nothing is pressed, the roller wheels won't move\n      Roller.stop();\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted5() {\n  Intake.setVelocity(300.0, percent);\n  Intake.setMaxTorque(300.0, percent);\n  Flywheel.setVelocity(300.0, percent);\n  Flywheel.setMaxTorque(300.0, percent);\n  Flywheel2.setVelocity(300.0, percent);\n  Flywheel2.setMaxTorque(300.0, percent);\n  while (true) {\n    // movement of robot\n    // controller position allows for all direction movement\n    FL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    FL.spin(forward);\n    BL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    BL.spin(forward);\n    FR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    FR.spin(forward);\n    BR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    BR.spin(forward);\n    if (Controller1.ButtonR2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move into the robot\n      Intake.spin(forward);\n    } else if (Controller1.ButtonL2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move away from the robot\n      Intake.spin(reverse);\n    } else {\n      // when nothing is pressed, the intake wheels won't move\n      Intake.stop();\n    }\n    if (Controller1.ButtonA.pressing()) {\n      // When A is pressed on the controller, the wheels will spin away from the robot\n      Roller.spin(forward);\n    } else if (Controller1.ButtonB.pressing()) {\n      // When B is press on the controller, the wheels will spin into the robot\n      Roller.spin(reverse);\n    } else {\n      // when nothing is pressed, the roller wheels won't move\n      Roller.stop();\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted6() {\n  while (true) {\n    // movement of robot\n    // controller position allows for all direction movement\n    FL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    FL.spin(forward);\n    BL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    BL.spin(forward);\n    FR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    FR.spin(forward);\n    BR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    BR.spin(forward);\n    if (Controller1.ButtonR2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move into the robot\n      Intake.spin(forward);\n    } else if (Controller1.ButtonL2.pressing()) {\n      // when r2 is pressed on the controller, the wheels will move away from the robot\n      Intake.spin(reverse);\n    } else {\n      // when nothing is pressed, the intake wheels won't move\n      Intake.stop();\n    }\n    if (Controller1.ButtonA.pressing()) {\n      // When A is pressed on the controller, the wheels will spin away from the robot\n      Roller.spin(forward);\n    } else if (Controller1.ButtonB.pressing()) {\n      // When B is press on the controller, the wheels will spin into the robot\n      Roller.spin(reverse);\n    } else {\n      // when nothing is pressed, the roller wheels won't move\n      Roller.stop();\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted7() {\n  // Pneumatics Code\n  while (true) {\n    if (Controller1.ButtonUp.pressing()) {\n      // When Up is pressed on the D-Pad, it will activate the piston\n      PB.set(true);\n    }\n    else {\n      // When nothing is pressed, it will deactivate the piston\n      PB.set(false);\n    }\n    if (Controller1.ButtonR1.pressing()) {\n      // When R1 is pressed, it will activate the piston\n      PA.set(true);\n    }\n    else {\n      // When nothing is pressed, it will deactivate the piston\n      PA.set(false);\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted8() {\n  while (true) {\n    if (Controller1.ButtonL1.pressing()) {\n      // if L1 is pressed on the controller, it will spin the flywheel away from the robot\n      Flywheel.spin(reverse);\n    }\n    else {\n      if (Controller1.ButtonDown.pressing()) {\n        // if Down is pressed on the controller's D-pad, it will stop the flywheel\n        Flywheel.stop();\n      }\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted9() {\n  while (true) {\n    if (flywheel == 1.0) {\n      Flywheel.spin(reverse);\n      Flywheel2.spin(reverse);\n    }\n    else {\n      if (flywheel == 0.0) {\n        Flywheel.stop();\n        Flywheel2.stop();\n      }\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted10() {\n  while (true) {\n    if (Controller1.ButtonL1.pressing()) {\n      // if L1 is pressed on the controller, it will spin the flywheel away from the robot\n      flywheel = 1.0;\n    }\n    else {\n      if (Controller1.ButtonDown.pressing()) {\n        // if Down is pressed on the controller's D-pad, it will stop the flywheel\n        flywheel = 0.0;\n      }\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n\nint main() {\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  vex::task ws1(whenStarted2);\n  vex::task ws2(whenStarted3);\n  vex::task ws3(whenStarted4);\n  vex::task ws4(whenStarted5);\n  vex::task ws5(whenStarted6);\n  vex::task ws6(whenStarted7);\n  vex::task ws7(whenStarted8);\n  vex::task ws8(whenStarted9);\n  vex::task ws9(whenStarted10);\n  whenStarted1();\n}","target":"Physical"}