                                                 \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\n/*vex-vision-config:begin*/\nvision::signature Vision12__BLUEBOX = vision::signature (1, -1489, -1091, -1290,13889, 14359, 14124,3, 0);\nvision::signature Vision12__REDBOX = vision::signature (2, 5193, 6063, 5628,-1019, -709, -864,7.8, 0);\nvision::signature Vision12__GREENBOX = vision::signature (3, -5573, -5213, -5394,-6003, -5651, -5826,6.5, 0);\nvision Vision12 = vision (PORT12, 50, Vision12__BLUEBOX, Vision12__REDBOX, Vision12__GREENBOX);\n/*vex-vision-config:end*/\n\n\n\n\n\n// Helper to make playing sounds from the V5 in VEXcode easier and\n// keeps the code cleaner by making it clear what is happening.\nvoid playVexcodeSound(const char *soundName) {\n  printf(\"VEXPlaySound:%s\\n\", soundName);\n  wait(5, msec);\n}\n\n\n\n// Generated code.\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0, Console_precision = 0;\n\nfloat myVariable;\n\nevent checkRed = event();\nevent checkBlue = event();\nevent checkGreen = event();\nevent message1 = event();\n\n// \"when started\" hat block\nint whenStarted1() {\n  while (true) {\n    checkBlue.broadcastAndWait();\n    checkRed.broadcastAndWait();\n    checkGreen.broadcastAndWait();\n    wait(0.1, seconds);\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when I receive checkBlue\" hat block\nvoid onevent_checkBlue_0() {\n  Brain.Screen.setFont(mono40);\n  Brain.Screen.clearLine(1);\n  Brain.Screen.setCursor(Brain.Screen.row(), 1);\n  Brain.Screen.setCursor(1, 1);\n  Vision12.takeSnapshot(Vision12__BLUEBOX);\n  if (Vision12.objectCount > 0) {\n    Brain.Screen.print(\"Blue Object Found\");\n  }\n  else {\n    Brain.Screen.print(\"No Blue Object\");\n  }\n}\n\n// \"when I receive checkRed\" hat block\nvoid onevent_checkRed_0() {\n  Brain.Screen.setFont(mono40);\n  Brain.Screen.clearLine(3);\n  Brain.Screen.setCursor(Brain.Screen.row(), 1);\n  Brain.Screen.setCursor(3, 1);\n  Vision12.takeSnapshot(Vision12__REDBOX);\n  if (Vision12.objectCount > 0) {\n    Brain.Screen.print(\"Red Object Found\");\n  }\n  else {\n    Brain.Screen.print(\"No Red Object\");\n  }\n}\n\n// \"when I receive checkGreen\" hat block\nvoid onevent_checkGreen_0() {\n  Brain.Screen.setFont(mono40);\n  Brain.Screen.clearLine(5);\n  Brain.Screen.setCursor(Brain.Screen.row(), 1);\n  Brain.Screen.setCursor(5, 1);\n  Vision12.takeSnapshot(Vision12__GREENBOX);\n  if (Vision12.objectCount > 0) {\n    Brain.Screen.print(\"Green Object Found\");\n  }\n  else {\n    Brain.Screen.print(\"No Green Object\");\n  }\n}\n\n\nint main() {\n  // register event handlers\n  checkBlue(onevent_checkBlue_0);\n  checkRed(onevent_checkRed_0);\n  checkGreen(onevent_checkGreen_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}