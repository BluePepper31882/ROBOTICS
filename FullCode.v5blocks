                          \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\ncontroller Controller1 = controller(primary);\ndigital_out PA = digital_out(Brain.ThreeWirePort.A);\nmotor FL = motor(PORT20, ratio18_1, false);\n\nmotor FR = motor(PORT19, ratio18_1, false);\n\nmotor BL = motor(PORT18, ratio18_1, false);\n\nmotor BR = motor(PORT17, ratio18_1, false);\n\nmotor Intake = motor(PORT15, ratio18_1, false);\n\nmotor Roller = motor(PORT16, ratio18_1, false);\n\nmotor Flywheel = motor(PORT11, ratio6_1, false);\n\n\n\n\n\n\n// Helper to make playing sounds from the V5 in VEXcode easier and\n// keeps the code cleaner by making it clear what is happening.\nvoid playVexcodeSound(const char *soundName) {\n  printf(\"VEXPlaySound:%s\\n\", soundName);\n  wait(5, msec);\n}\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat myVariable, flywheel;\n\n// \"when started\" hat block\nint whenStarted1() {\n  Roller.setMaxTorque(200.0, percent);\n  Intake.setVelocity(300.0, percent);\n  Intake.setMaxTorque(300.0, percent);\n  Flywheel.setVelocity(300.0, percent);\n  while (true) {\n    // Movement of robot\n    // Controller position allows for all direction movement\n    FL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    FL.spin(forward);\n    BL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    BL.spin(forward);\n    FR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    FR.spin(forward);\n    BR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    BR.spin(forward);\n    if (Controller1.ButtonR2.pressing()) {\n      // When R2 is pressed on the controller, the intake wheels will spin into the robot\n      Intake.spin(forward);\n    } else if (Controller1.ButtonL2.pressing()) {\n      // When R2 is pressed on the controller, the intake wheels will spin away from the robot\n      Intake.spin(reverse);\n    } else {\n      // When nothing is pressed, the intake wheels wont move\n      Intake.stop();\n    }\n    if (Controller1.ButtonA.pressing()) {\n      // When A is pressed on the controller, the roller wheels will spin away from the robot\n      Roller.spin(forward);\n    } else if (Controller1.ButtonB.pressing()) {\n      // When B is pressed on the controller, the roller wheels will spin into the robot\n      Roller.spin(reverse);\n    } else {\n      // When nothing is pressed, the roller wheels wont move\n      Roller.stop();\n    }\n    if (Controller1.ButtonL1.pressing()) {\n      // When L1 is pressed, the flywheel variable will activate\n      flywheel = 1.0;\n    }\n    else {\n      if (Controller1.ButtonDown.pressing()) {\n        // When down is pressed, the flywheel variable will deactivate\n        flywheel = 0.0;\n      }\n    }\n    FL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    FL.spin(forward);\n    BL.setVelocity((Controller1.Axis1.position() + (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    BL.spin(forward);\n    FR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() - Controller1.Axis4.position())), percent);\n    FR.spin(forward);\n    BR.setVelocity((Controller1.Axis1.position() - (Controller1.Axis3.position() + Controller1.Axis4.position())), percent);\n    BR.spin(forward);\n    if (Controller1.ButtonR2.pressing()) {\n      // When R2 is pressed on the controller, the intake wheels will spin into the robot\n      Intake.spin(forward);\n    } else if (Controller1.ButtonL2.pressing()) {\n      // When R2 is pressed on the controller, the intake wheels will spin away from the robot\n      Intake.spin(reverse);\n    } else {\n      // When nothing is pressed, the intake wheels wont move\n      Intake.stop();\n    }\n    if (Controller1.ButtonA.pressing()) {\n      // When A is pressed on the controller, the roller wheels will spin away from the robot\n      Roller.spin(forward);\n    } else if (Controller1.ButtonB.pressing()) {\n      // When B is pressed on the controller, the roller wheels will spin into the robot\n      Roller.spin(reverse);\n    } else {\n      // When nothing is pressed, the roller wheels wont move\n      Roller.stop();\n    }\n    if (Controller1.ButtonL1.pressing()) {\n      // When L1 is pressed, the flywheel variable will activate\n      flywheel = 1.0;\n    }\n    else {\n      if (Controller1.ButtonDown.pressing()) {\n        // When down is pressed, the flywheel variable will deactivate\n        flywheel = 0.0;\n      }\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted2() {\n  // Pneumatics Code\n  while (true) {\n    // when R1 is pressed, pneumatic solonoid is opened for piston\n    if (Controller1.ButtonR1.pressing()) {\n      PA.set(true);\n    }\n    else {\n      PA.set(false);\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted3() {\n  // speeds up robot if needed\n  while (true) {\n    if (Controller1.ButtonY.pressing()) {\n      BR.setVelocity(300.0, percent);\n      BL.setVelocity(300.0, percent);\n      FR.setVelocity(300.0, percent);\n      FL.setVelocity(300.0, percent);\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when started\" hat block\nint whenStarted4() {\n  // flywheel variable code\n  while (true) {\n    if (flywheel == 1.0) {\n      // when activated, the flywheel will continuously spin clockwise\n      Flywheel.spin(forward);\n    }\n    else {\n      if (flywheel == 0.0) {\n        // when deactivated, the flywheel will stop moving\n        Flywheel.stop();\n      }\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n\nint main() {\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  vex::task ws1(whenStarted2);\n  vex::task ws2(whenStarted3);\n  vex::task ws3(whenStarted4);\n  whenStarted1();\n}","target":"Physical"}